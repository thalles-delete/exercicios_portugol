programa {
  funcao inicio() {
    // Escreva um código que mostre uma mensagem se é maior de idade ou não.

    inteiro idade
    escreva("Qual a sua idade? ")
    leia(idade)

    se (idade >= 18)
      escreva("Você é maior de idade.")
    senao se (idade <=-1)
      escreva("Você está vivo?")
    senao
      escreva("Você não é maior de idade.")
  }
}
